<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.willianhdz.proyecto_final_daute">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_cate"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/ic_cate"
        android:supportsRtl="true"
        android:theme="@style/Theme.Proyecto_Final_DAUTE">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.list_recycle_pro.DetalleProducto" />
        <activity android:name=".ui.list_recycle_usu.DetalleUsuario" />
        <activity android:name=".ui.list_recycle_cate.DetalleCategoria" />


        <activity android:name=".ui.producto.Edit_Producto" />
        <activity android:name=".ui.categoria.Edit_Categoria" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >

        </activity>
        <activity android:name=".ui.usuario.Edit_Usuario" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

</manifest>